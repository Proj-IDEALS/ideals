<p style="color: green"><%= notice %></p>

<h1>Practices</h1>

<div id="practices">
  <% @practices.each do |practice| %>
    <strong><%= practice.name %></strong>
    <p>
      <%= link_to "Show this practice", practice %>
    </p>
  <% end %>
</div>

<%= link_to "New practice", new_practice_path %>
